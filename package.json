{
  "name": "doc-components",
  "version": "0.0.2",
  "description": "React components that help you build documentation sites for other React components.",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist es tmp lib",
    "prepublish": "in-publish && npm run build || not-in-publish",
    "build": "npm run clean && npm run build:umd && npm run build:umd:min && npm run build:es && npm run build:commonjs",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env NODE_ENV=development BABEL_ENV=build rollup -c -i src/index.js -o dist/doc-components.js",
    "build:umd:min": "cross-env NODE_ENV=production BABEL_ENV=buildProd rollup -c -i src/index.js -o dist/doc-components.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesplease/doc-components.git"
  },
  "keywords": [
    "react",
    "components",
    "documentation",
    "docs",
    "github",
    "pages",
    "create-react-app",
    "markdown",
    "text",
    "editor",
    "source",
    "code"
  ],
  "author": "James Please <jamesplease2@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamesplease/doc-components/issues"
  },
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "dependencies": {
    "codemirror": "^5.36.0",
    "fetch-dedupe": "^3.0.0-beta1",
    "prop-types": "^15.6.1"
  },
  "peerDependencies": {
    "@babel/standalone": "^7.0.0-beta.42",
    "react": "^16.0.0",
    "react-dom": "^16.2.0",
    "react-lowlight": "^2.0.0",
    "react-markdown": "^3.3.0"
  },
  "devDependencies": {
    "@babel/standalone": "^7.0.0-beta.42",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.1.3",
    "in-publish": "^2.0.0",
    "rimraf": "^2.6.2",
    "react": "^16.0.0",
    "react-dom": "^16.2.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  }
}
