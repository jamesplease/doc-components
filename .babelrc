{
  "env": {
    "commonjs": {
      "plugins": [
        ["transform-es2015-modules-commonjs", { "loose": true }],
        "transform-class-properties",
        "transform-es2015-classes"
      ],
      "presets": ["stage-3", "react"]
    },
    "test": {
      "presets": ["env", "stage-3", "react"],
      "plugins": ["transform-class-properties"]
    }
  }
}
